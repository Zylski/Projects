<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <display-name>Utopia</display-name>
    <servlet>
        <servlet-name>Utopia</servlet-name>
        <servlet-class>com.shinra.utopia.login.UtopiaLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UtopiaLogin</servlet-name>
        <servlet-class>com.shinra.utopia.login.UtopiaLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>com.shinra.utopia.admin.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminFlightServlet</servlet-name>
        <servlet-class>com.shinra.utopia.admin.AdminFlightServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Utopia</servlet-name>
        <url-pattern>/Utopia</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UtopiaLogin</servlet-name>
        <url-pattern>/UtopiaLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminFlightServlet</servlet-name>
        <url-pattern>/AdminFlightServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
